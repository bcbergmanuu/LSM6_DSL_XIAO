/ {
    zephyr,user {
        signal-gpios = <&gpio0 11 GPIO_PULL_UP>;
    };

    chosen {
        nordic,pm-ext-flash = &p25q16h;
    };
};

&zephyr_udc0 {
	cdc_acm_uart0 {
		compatible = "zephyr,cdc-acm-uart";
	};
};

&i2c0 {	
    lsm6ds3tr_c: lsm6ds3tr-c@6a {
        status = "disabled";
    };
};
